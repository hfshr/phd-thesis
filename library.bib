keywords = {heart rate variability,subband analysis,wavelet packet decomposition},
number = {December 2014},
pages = {105--111},
publisher = {Elsevier B.V.},
title = {{Wavelet transform based HRV analysis}},
url = {http://dx.doi.org/10.1016/j.protcy.2013.12.462{\%}0Ahttp://ac.els-cdn.com/S2212017313006476/1-s2.0-S2212017313006476-main.pdf?{\_}tid=8941ead6-9824-11e7-9778-00000aab0f02{\&}acdnat=1505267014{\_}14fa3d485d21743f69e69ba97dedc2a5},
