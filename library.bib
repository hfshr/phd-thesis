isbn = {1879-1271 (Electronic)$\backslash$r0268-0033 (Linking)},
issn = {18791271},
journal = {Clinical Biomechanics},
keywords = {Anterior cruciate ligament injury,Balance,Double-leg stance,Postural stability,Single-leg stance},
