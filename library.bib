issn = {18733360},
journal = {Psychoneuroendocrinology},
keywords = {Acute stress,Cortisol,Emotion regulation,HPA axis,Relationship stress,Suppression},
month = {mar},
pages = {69--77},
pmid = {29331801},
publisher = {Pergamon},
title = {{Stressful life events, relationship stressors, and cortisol reactivity: The moderating role of suppression}},
url = {https://www.sciencedirect.com/science/article/pii/S030645301730433X},
