file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hainaut et al. - 2011 - The role of trait anxiety on static balance performance in control and anxiogenic situations.pdf:pdf},
