doi = {10.1136/bjsm.2006.026609},
file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/H{\"{a}}gglund, Wald{\'{e}}n, Ekstrand - 2006 - Previous injury as a risk factor for injury in elite football A prospective study over two consecuti.pdf:pdf},
