keywords = {elasticity,elastography,followup,neuromuscular disorders,stiffness},
month = {mar},
number = {3},
pages = {19--28},
pmid = {22370174},
title = {{Supersonic shear imaging provides a reliable measurement of resting muscle shear elastic modulus}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22370174 http://stacks.iop.org/0967-3334/33/i=3/a=N19?key=crossref.131d7e0f407027ceb5a2bb4502866ed5},
