doi = {10.2519/jospt.2017.6974},
file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Pickett et al. - 2017 - Star Excursion Balance Test Anterior Asymmetry Is Associated With Injury Status in Division I Collegiate Athlete.pdf:pdf},
