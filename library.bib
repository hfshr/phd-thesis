title = {{Previous injury as a risk factor for injury in elite football: A prospective study over two consecutive seasons}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16855067 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2564391},
