doi = {10.1136/bjsm.2004.016188},
file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/McIntosh - 2005 - Risk compensation, motivation, injuries, and biomechanics in competitive sport.pdf:pdf},
