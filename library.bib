isbn = {0198522703},
issn = {14691825},
journal = {Behavioral and Brain Sciences},
keywords = {Papez circuit,antianxiety drugs,anxiety,arousal,ascending noradrenergic pathways,ascending serotonergic pathways,attention,comparator,depression,personality,prefrontal cortex,septo-hippocampal system (SHS)},
month = {sep},
number = {3},
pages = {469--484},
pmid = {1000101789},
title = {{Pr{\'{e}}cis of the neuropsychology of anxiety: An enquiry into the functions of the septo-hippocampal system}},
