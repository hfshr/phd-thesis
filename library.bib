journal = {International Journal of Athletic Therapy and Training},
keywords = {Clinical prediction guide,Injury prevention,Injury risk},
number = {6},
pages = {4--9},
title = {{Neurocognitive reaction time predicts lower extremity sprains and strains}},
volume = {17},
year = {2012}
