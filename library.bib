isbn = {0301-0511},
issn = {03010511},
journal = {Biological Psychology},
keywords = {Cardiac vagal control,Cardiac vagal tone,Heart rate variability,Methodology,Respiration,Respiratory sinus arrhythmia,Vagus},
number = {2},
pages = {286--294},
pmid = {17067734},
title = {{Methodological issues in the quantification of respiratory sinus arrhythmia}},
