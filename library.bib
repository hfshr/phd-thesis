file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Calvi et al. - 2017 - Measurement of cortisol in saliva A comparison of measurement error within and between international academic-rese.pdf:pdf},
isbn = {1756-0500},
issn = {17560500},
journal = {BMC Research Notes},
keywords = {Inter-assay,Inter-laboratory variation,Intra-assay,Salivary cortisol},
