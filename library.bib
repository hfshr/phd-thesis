file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee et al. - 2017 - Biomarkers in sports and exercise Tracking health, performance, and recovery in athletes.pdf:pdf},
isbn = {0000000000},
issn = {15334295},
journal = {Journal of Strength and Conditioning Research},
keywords = {Endurance performance,Hydration,Inflammation,Injury prevention,Muscle quality},
number = {10},
pages = {2920--2937},
pmid = {28737585},
publisher = {Wolters Kluwer Health},
title = {{Biomarkers in sports and exercise: Tracking health, performance, and recovery in athletes}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28737585},
volume = {31},
year = {2017}
}
@article{Hauck2008,
