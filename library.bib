journal = {Clinical Endocrinology},
month = {sep},
number = {3},
pages = {336--341},
pmid = {16117823},
title = {{Salivary cortisol determined by enzyme immunoassay is preferable to serum total cortisol for assessment of dynamic hypothalamic-pituitary-adrenal axis activity}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16117823 http://doi.wiley.com/10.1111/j.1365-2265.2005.02349.x},
volume = {63},
year = {2005}
}
