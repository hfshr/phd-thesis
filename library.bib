file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Kucera et al. - 2005 - Injury history as a risk factor for incident injury in youth soccer.pdf:pdf},
issn = {03063674},
