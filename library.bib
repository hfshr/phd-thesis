file = {:C$\backslash$:/Users/Harry/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Robertson, Beveridge, Bromley - 2017 - Allostatic load as a predictor of all-cause and cause-specific mortality in the general populatio.pdf:pdf},
issn = {19326203},
journal = {PLoS ONE},
